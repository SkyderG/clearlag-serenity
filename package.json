{
  "name": "clearlag",
  "version": "1.0.2",
  "author": "Skyder",
  "description": "A plugin to clear entities and items from the world to reduce lag.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "license": "MIT",
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs"
    ],
    "dts": true,
    "clean": true,
    "minify": true
  },
  "keywords": [
    "Management",
    "Optimization"
  ],
  "peerDependencies": {
    "@serenityjs/core": "*",
    "@serenityjs/plugins": "*"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
